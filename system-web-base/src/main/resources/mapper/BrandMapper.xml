<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sw.base.mapper.goods.BrandMapper">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.sw.common.entity.goods.Brand" id="brandMap">
        <result property="pkBrandId" column="pk_brand_id"/>
        <result property="brandName" column="brand_name"/>
        <result property="brandNo" column="brand_no"/>
        <result property="brandType" column="brand_type"/>
        <result property="isDelete" column="is_delete"/>
        <result property="addUser" column="add_user"/>
        <result property="addTime" column="add_time"/>
        <result property="updateUser" column="update_user"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        pk_brand_id
        brand_name
        brand_no
        brand_type
        is_delete
        add_user
        add_time
        update_user
        update_time
    </sql>

</mapper>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sw.base.mapper.goods.SkuMapper">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.sw.common.entity.goods.Sku" id="skuMap">
        <result property="pkSkuId" column="pk_sku_id"/>
        <result property="fkGoodsId" column="fk_goods_id"/>
        <result property="skuName" column="sku_name"/>
        <result property="skuCodee" column="sku_codee"/>
        <result property="skuBarCode" column="sku_bar_code"/>
        <result property="skuNum" column="sku_num"/>
        <result property="skuStock" column="sku_stock"/>
        <result property="skuPrice" column="sku_price"/>
        <result property="skuStatus" column="sku_status"/>
        <result property="isDelete" column="is_delete"/>
        <result property="addUser" column="add_user"/>
        <result property="addTime" column="add_time"/>
        <result property="updateUser" column="update_user"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        pk_sku_id
        fk_goods_id
        sku_name
        sku_codee
        sku_bar_code
        sku_num
        sku_stock
        sku_price
        sku_status
        is_delete
        add_user
        add_time
        update_user
        update_time
    </sql>

</mapper>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sw.base.mapper.goods.SpecOptionMapper">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.sw.common.entity.goods.SpecOption" id="specOptionMap">
        <result property="pkSpecOptionId" column="pk_spec_option_id"/>
        <result property="fkSpecsId" column="fk_specs_id"/>
        <result property="name" column="name"/>
        <result property="code" column="code"/>
        <result property="isDelete" column="is_delete"/>
        <result property="addUser" column="add_user"/>
        <result property="addTime" column="add_time"/>
        <result property="updateUser" column="update_user"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        pk_spec_option_id
        fk_specs_id
        name
        code
        is_delete
        add_user
        add_time
        update_user
        update_time
    </sql>

</mapper>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sw.base.mapper.system.JobMapper">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.sw.common.entity.system.Job" id="jobMap">
        <result property="pkJobId" column="pk_job_id"/>
        <result property="jobName" column="job_name"/>
        <result property="methodName" column="method_name"/>
        <result property="corn" column="corn"/>
        <result property="status" column="status"/>
        <result property="className" column="class_name"/>
        <result property="jobGroup" column="job_group"/>
        <result property="description" column="description"/>
        <result property="springBean" column="spring_bean"/>
        <result property="isDelete" column="is_delete"/>
        <result property="addUser" column="add_user"/>
        <result property="addTime" column="add_time"/>
        <result property="updateUser" column="update_user"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        pk_job_id
        job_name
        method_name
        corn
        status
        class_name
        job_group
        description
        spring_bean
        is_delete
        add_user
        add_time
        update_user
        update_time
    </sql>

</mapper>
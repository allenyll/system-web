<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sw.base.mapper.goods.SizeMapper">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.sw.common.entity.goods.Size" id="sizeMap">
        <result property="pkSizeId" column="pk_size_id"/>
        <result property="fkSizeGroupId" column="fk_size_group_id"/>
        <result property="sizeName" column="size_name"/>
        <result property="sizeCode" column="size_code"/>
        <result property="isUsed" column="is_used"/>
        <result property="isDefault" column="is_default"/>
        <result property="isDelete" column="is_delete"/>
        <result property="addUser" column="add_user"/>
        <result property="addTime" column="add_time"/>
        <result property="updateUser" column="update_user"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        pk_size_id
        fk_size_group_id
        size_name
        size_code
        is_used
        is_default
        is_delete
        add_user
        add_time
        update_user
        update_time
    </sql>

</mapper>